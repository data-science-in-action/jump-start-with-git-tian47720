mkdir learn_git_again
cd learn_git_again
touch third.txt
git init
git add third.txt
git commit -m "AddThird.txt"
git log
touch fourth.txt
git add fourth.txt
git commit -m "AddFourth.txt"
rm third.txt
git add third.txt
git commit -m "移除第三方txt"
git log
git config --global core.pager "cat"
git config --global --list
